{
    "name": "my-job-journal",
    "version": "1.0.0",
    "description": "An app to help you keep track of your own job hunt. ",
    "main": "server.js",
    "engines": {
        "node": "8.2.1",
        "npm": "5.3.0"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "server": "nodemon server.js",
        "client": "npm run start --prefix client",
