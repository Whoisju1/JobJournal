    "dependencies": {
        "and": "0.0.0",
        "axios": "^0.16.2",
        "body-parser": "^1.17.2",
        "concurrently": "^3.5.0",
        "connect-mongo": "^1.3.2",
        "eslint": "^4.13.1",
        "express": "^4.15.3",
        "express-session": "^1.15.5",
        "graphql": "^0.13.0",
        "mongoose": "^4.11.5",
        "passport": "^0.3.2",
